@model PageViewModel
@{
    ViewData["Title"] = Model.SeoTitle;
}
<div id="wrapper-content">
    <!-- PAGE WRAPPER-->
    <div id="page-wrapper">
        <!-- MAIN CONTENT-->
        <div class="main-content">
            <!-- CONTENT-->
            <div class="content">
                <div class="section section-padding contact-main">
                    <div class="container">
                        <div class="contact-main-wrapper">
                            <div class="row contact-method">
                                @Html.Raw(Model.Body)
                                @await Component.InvokeAsync("EmbedForm", new { name = "İletişim", template = "ContactForm" })
                            </div>
                        </div>
                    </div>
                </div>
                <div id="map" class="section contact-map"><iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3011.649868864954!2d29.026767015182685!3d40.9891471285511!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14cab86f75b5800d%3A0x2d0fcf9b3fb8d79d!2sBili%C5%9Fim+E%C4%9Fitim+Merkezi!5e0!3m2!1str!2str!4v1493136508063" width="100%" height="600" frameborder="0" style="border:0" allowfullscreen></iframe></div>
            </div>
        </div>
        <!-- BUTTON BACK TO TOP-->
        <div id="back-top"><a href="#top"><i class="fa fa-angle-double-up"></i></a></div>
    </div>
